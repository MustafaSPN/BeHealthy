<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundcolor"
    tools:context=".Workout">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:srcCompat="@mipmap/tr_round" />

        <Switch
            android:id="@+id/workoutSwitch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:srcCompat="@mipmap/eng" />

    </LinearLayout>

    <TextView
        android:id="@+id/workout_text"
        android:layout_width="410dp"
        android:layout_height="90dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="2dp"
        android:fontFamily="@font/baloo"
        android:gravity="center"
        android:text="TextView"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginBottom="48dp"
        android:fontFamily="@font/baloo"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/wotext1"
        app:layout_constraintTop_toBottomOf="@+id/workout_text" />

    <Button
        android:id="@+id/button4"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginBottom="68dp"
        android:fontFamily="@font/baloo"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/wotext2" />

    <Button
        android:id="@+id/button6"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginBottom="68dp"
        android:fontFamily="@font/baloo"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/wotext4" />

    <Button
        android:id="@+id/button7"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginBottom="48dp"
        android:fontFamily="@font/baloo"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/wotext5" />

    <Button
        android:id="@+id/button5"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginBottom="68dp"
        android:fontFamily="@font/baloo"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/wotext3" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="109dp"
        android:layout_height="103dp"
        android:layout_marginStart="8dp"

        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView9"

        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/denemee_foreground" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="109dp"
        android:layout_height="103dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView10"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/deneme3_foreground" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="109dp"
        android:layout_height="103dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView11"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/deneme2_foreground" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="109dp"
        android:layout_height="103dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView12"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/deneme4_foreground" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="109dp"
        android:layout_height="103dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="12dp"
        android:scaleType="matrix"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/deneme5_foreground" />

    <TextView
        android:id="@+id/wotext1"
        android:layout_width="137dp"
        android:layout_height="48dp"
        android:layout_marginBottom="48dp"
        android:fontFamily="@font/baloo"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/wotext2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/workout_text" />

    <TextView
        android:id="@+id/wotext2"
        android:layout_width="137dp"
        android:layout_height="48dp"
        android:layout_marginBottom="64dp"
        android:fontFamily="@font/baloo"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/wotext3"
        app:layout_constraintEnd_toEndOf="@+id/wotext1"
        app:layout_constraintStart_toStartOf="@+id/wotext1" />

    <TextView
        android:id="@+id/wotext3"
        android:layout_width="137dp"
        android:layout_height="48dp"
        android:layout_marginBottom="64dp"
        android:fontFamily="@font/baloo"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/wotext4"
        app:layout_constraintEnd_toEndOf="@+id/wotext2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/wotext2" />

    <TextView
        android:id="@+id/wotext4"
        android:layout_width="137dp"
        android:layout_height="48dp"
        android:layout_marginBottom="64dp"
        android:fontFamily="@font/baloo"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/wotext5"
        app:layout_constraintEnd_toEndOf="@+id/wotext3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/wotext3" />

    <TextView
        android:id="@+id/wotext5"
        android:layout_width="137dp"
        android:layout_height="48dp"
        android:layout_marginBottom="48dp"
        android:fontFamily="@font/baloo"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="@+id/wotext4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/wotext4" />

</androidx.constraintlayout.widget.ConstraintLayout>